// classes example
#include <iostream>
using namespace std;

class rectangle-qwe {
    int width, height;
  public:
    void set_values (int,int);
    int area() {return width*height;}
};

void rectangle-qwe::set_values (int x, int y) {
  width = x;
  height = y;
}

int main () {
  rectangle-qwe rect;
  rect.set_values (3,4);
  cout << "area: " << rect.area() << "\n";
  return 0;
}
